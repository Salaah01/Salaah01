{
  "name": "visualcsv",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run dev:data_loader\" \"npm run dev:graph_builder\" \"npm run watch:main_css\" \"npm run watch:accounts_css\"",
    "build": "npm-run-all build:react build:css",
    "build:react": "npm-run-all build:data_loader build:graph_builder",
    "build:css": "npm-run-all build:main_css build:accounts_css",
    "dev:data_loader": "webpack --mode development --watch ./visualCSV/data_loader/src/index.js --output ./visualCSV/data_loader/static/data_loader/main.js",
    "build:data_loader": "webpack --mode production ./visualCSV/data_loader/src/index.js --output ./visualCSV/data_loader/static/data_loader/main.js",
    "dev:graph_builder": "webpack --mode development --watch ./visualCSV/graph_builder/src/index.js --output ./visualCSV/graph_builder/static/graph_builder/main.js",
    "build:graph_builder": "webpack --mode production ./visualCSV/graph_builder/src/index.js --output ./visualCSV/graph_builder/static/graph_builder/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "compile:main_css": "node-sass visualCSV/visualCSV/static/sass/main.scss visualCSV/visualCSV/static/css/main.min.css",
    "watch:main_css": "node-sass visualCSV/visualCSV/static/sass/main.scss visualCSV/visualCSV/static/css/main.min.css -w --output-style compressed",
    "prefix:main_css": "postcss --use autoprefixer -b \"last 10 versions\" visualCSV/visualCSV/static/css/main.min.css -o visualCSV/visualCSV/static/css/main.min.css",
    "compress:main_css": "node-sass visualCSV/visualCSV/static/css/main.min.css visualCSV/visualCSV/static/css/main.min.css --output-style compressed",
    "build:main_css": "npm-run-all compile:main_css prefix:main_css compress:main_css",
    "watch:accounts_css": "node-sass visualCSV/accounts/static/accounts/sass/accounts.scss visualCSV/accounts/static/accounts/css/accounts.min.css -w --output-style compressed",
    "compile:accounts_css": "node-sass visualCSV/accounts/static/accounts/sass/accounts.scss visualCSV/accounts/static/accounts/css/accounts.min.css",
    "prefix:accounts_css": "postcss --use autoprefixer -b \"last 10 versions\" visualCSV/accounts/static/accounts/css/accounts.min.css -o visualCSV/accounts/static/accounts/css/accounts.min.css",
    "compress:accounts_css": "node-sass visualCSV/accounts/static/accounts/css/accounts.min.css visualCSV/accounts/static/accounts/css/accounts.min.css --output-style compressed",
    "build:accounts_css": "npm-run-all compile:accounts_css prefix:accounts_css compress:accounts_css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Salaah01/visualCSV.git"
  },
  "author": "Salaah Amin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Salaah01/visualCSV/issues"
  },
  "homepage": "https://github.com/Salaah01/visualCSV#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-react-app": "^9.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-equimper": "^2.2.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.2.7",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.6",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.9.3",
    "csv": "^5.3.2",
    "deepmerge": "^4.2.2",
    "django-react-csrftoken": "^1.0.4",
    "file-loader": "^6.0.0",
    "radium": "^0.26.0",
    "random-flat-colors": "^1.0.6",
    "react": "^16.7.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-chartjs-2": "^2.9.0",
    "react-color": "^2.18.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.7.0",
    "react-dropzone": "^10.2.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "browsers": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
